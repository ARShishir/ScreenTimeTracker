{
  "manifest_version": 3,
  "name": "Screen Time Tracker",
  "version": "1.0",
  "description": "Tracks browsing Screen Time, mouse events, and ranks websites by visit frequency and time spent.",
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "webNavigation", 
    "history"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "author": "Abdur Rahaman Shishir",
  "action": {
    "default_popup": "popup.html"
  }
  
}
